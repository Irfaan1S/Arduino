#define LED_PIN 13
void setup() {
  // put your setup code here, to run once:
  pinMode(LED_PIN,OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
  digitalWrite(LED_PIN,HIGH);
  delay(1000);  //delay is in milliseconds //which means for 1000 ms do not process any other Commands//LED is ON for how long here,1 sec
  digitalWrite(LED_PIN,LOW);
  delay(1000); //LED is OFF for how long here,1 sec
}

//LED will be ON for 1 sec and OFF for 1 sec
//void loop will work like a forever loop
